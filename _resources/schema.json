{
    "$schema": "http://json-schema.org/schema#",
    "type":"object",
    "properties":{
        "conformsTo":{
            "enum": [
                "http://codefordc.org/resources/specification.html",
                "https://github.com/codefordc/guides/blob/master/using-civic.json/specification.md"
            ]
        },
        "status":{
            "type":"string",
            "blank":true
        },
        "thumbnailUrl":{
            "type":"string",
            "blank":true
        },
        "contact":{
            "type":"object",
            "properties":{
                "name":{
                    "type":"string",
                    "blank":true
                },
                "email":{
                    "type":"string",
                    "blank":true
                },
                "twitter":{
                    "type":"string",
                    "blank":true
                }
            }
        },
        "bornAt":{
            "type":"string"
        },
        "geography":{
            "type":"string"
        },
        "politicalEntity":{
            "type":"object"
        },
        "governmentPartner":{
            "type":"object"
        },
        "communityPartner":{
            "type":"object"
        },
        "type":{
            "type":"string",
            "blank":true
        },
        "data":{
            "type":"object",
            "required":false
        },
        "needs":{
            "type":"array"
        },
        "categories":{
            "type":"array"
        },
        "moreInfo":{
            "type":"string",
            "blank":true
        }
    }
}
